        "github>rodent1/home-ops//.renovate/labels.json5",
        "github>rodent1/home-ops//.renovate/semanticCommits.json5",
        ":automergeBranch",
        ":disableRateLimiting",
        ":dependencyDashboard",
        ":semanticCommits",
        ":timezone(Europe/Oslo)",
