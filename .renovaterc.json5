{
    $schema: "https://docs.renovatebot.com/renovate-schema.json",
    extends: [
        "config:recommended",
        "docker:enableMajor",
        "helpers:pinGitHubActionDigests",
        "github>rodent1/home-ops//.renovate/autoMerge.json5",
